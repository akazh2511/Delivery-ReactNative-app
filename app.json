    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package":"com.akashdeliveryapp.delivery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
